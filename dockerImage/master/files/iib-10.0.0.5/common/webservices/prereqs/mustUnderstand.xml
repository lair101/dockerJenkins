<module name="mustUnderstand" class="com.ibm.broker.axis2.MustUnderstandHeaderModule">
  <Description>This is a handler for Must Understand headers the Broker shouldn't process.</Description>
  <InFlow>
    <handler name="MustUnderstandHeaderInHandler" class="com.ibm.broker.axis2.MustUnderstandHeaderInHandler">
      <order phase="Dispatch" phaseLast="true"/>
    </handler>
  </InFlow>
  <InFaultFlow>
    <handler name="MustUnderstandHeaderFaultHandler" class="com.ibm.broker.axis2.MustUnderstandHeaderFaultHandler">
      <order phase="Dispatch" phaseLast="true"/>
    </handler>
  </InFaultFlow>
  <!-- <OutFlow>
    <handler name="MustUnderstandHeaderOutHandler" class="com.ibm.broker.axis2.MustUnderstandHeaderOutHandler">
      <order phase="MessageOut" phaseFirst="true"/>
    </handler>  
  </OutFlow> -->
</module> 
