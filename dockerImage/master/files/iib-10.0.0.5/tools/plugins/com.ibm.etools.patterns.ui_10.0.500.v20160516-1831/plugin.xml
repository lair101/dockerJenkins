<plugin>

<!-- Editors -->
<extension point="org.eclipse.ui.editors">
	<editor
       class="com.ibm.etools.patterns.editor.PatternGenerateEditor"
       contributorClass="com.ibm.etools.patterns.editor.PatternGenerateEditorContributor"
       default="false"
       extensions="xml"
       icon="icons/full/obj16/patternLeaf_obj.gif"
       id="com.ibm.etools.patterns.editor.PatternGenerate"
       name="%PatternInstance">
		</editor>
	</extension>
	
<extension point="org.eclipse.ui.editors">
	<editor name="%PatternSpecification"
			icon="icons/full/obj16/patternSpecification_obj.gif"
			class="com.ibm.etools.patterns.editor.PatternBrowsingEditor"	
			contributorClass="com.ibm.etools.patterns.editor.PatternBrowsingEditorContributor"
			id="com.ibm.etools.patterns.editor.PatternBrowse">
		</editor>		
	</extension>

<extension point="org.eclipse.ui.editors">
	<editor name="%PageViewerEditor.name"
			icon="icons/full/obj16/internal_browser.gif"
			class="com.ibm.etools.patterns.editor.PageViewerEditor"	
	        extensions="htm,html,shtml"
			id="com.ibm.etools.patterns.editor.PageViewerEditor">			
		</editor>
</extension>

