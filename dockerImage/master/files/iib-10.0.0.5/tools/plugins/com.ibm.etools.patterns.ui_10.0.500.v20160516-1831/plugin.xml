<!-- Patterns Explorer view for showing available Patterns-->
	<extension point="org.eclipse.ui.views">
	 <!-- Placing Patterns Explorer under Integration Patterns Category (Window-> Show View) -->
	 <category
            name="%ShowViewPatternCategory.name"
            id="com.ibm.etools.category.IntegrationPatterns">
      </category>
      
        <view id="com.ibm.etools.patterns.views.PatternExplorerView"
        	category="com.ibm.etools.category.IntegrationPatterns"
        	name="%PatternExplorerViewLabel"
        	class="com.ibm.etools.patterns.views.PatternExplorerView"
   	    	icon="icons/full/view16/patternsExplorerView_obj.gif"/>
	</extension>

	<extension point = "org.eclipse.ui.actionSets">
        <actionSet
            id="com.ibm.etools.patterns.PatternActionSet"
            label="Pattern Actions"
	    visible="true"
	    description="The action set for Pattern">

	    <action id="com.ibm.etools.patterns.action.OpenPatternViewAction"
		toolbarPath="Normal"			
		label="%CreatePattern"
		tooltip="%CreatePattern"
		icon="icons/full/obj16/startFrom_pattern.gif"
		class="com.ibm.etools.patterns.action.OpenPatternViewAction"/>
        </actionSet>
    </extension>

	   
