												<valueName>0</valueName>
												<displayName>0 - At most once</displayName>
											</enumValue>
											<enumValue>
												<valueName>1</valueName>
												<displayName>1 - At least once</displayName>
											</enumValue>
											<enumValue>
												<valueName>2</valueName>
												<displayName>2 - Exactly once</displayName>
											</enumValue>
										</enumValues>
									</values>
								</schema>
