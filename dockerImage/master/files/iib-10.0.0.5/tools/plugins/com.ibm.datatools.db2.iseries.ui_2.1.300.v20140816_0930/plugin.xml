       <!-- Override DTP table Data Edit menu  -->
  <extension point="org.eclipse.ui.navigator.navigatorContent">  
    <actionProvider
       class="com.ibm.datatools.db2.iseries.internal.ui.explorer.actions.popup.providers.DB2ISeriesEditTableDataActionProvider"
   	   id = "com.ibm.datatools.db2.iseries.internal.ui.tableDataEdit"   
   	   dependsOn="org.eclipse.datatools.sqltools.tabledataeditor.actions.popup.DataGroupProvider"	   
       overrides = "org.eclipse.datatools.sqltools.data.internal.edit">
       <enablement>
	        <and>
		        <or>
			       <adapt type="com.ibm.db.models.db2.iSeries.ISeriesTable" /> 
				</or>
			</and>
		</enablement>         
    </actionProvider>	
  </extension>
  
  <extension point="org.eclipse.ui.navigator.viewer">   
      <viewerActionBinding viewerId="org.eclipse.datatools.connectivity.DataSourceExplorerNavigator">
         <includes>
             <actionExtension pattern="com.ibm.datatools.db2.iseries.internal.ui.tableDataEdit" />             
         </includes>
      </viewerActionBinding>
  </extension>
  
  <!-- Contribute table data editor  -->
  <extension point="org.eclipse.ui.editors">
     <editor
        class = "com.ibm.datatools.db2.iseries.internal.ui.explorer.actions.popup.actions.DB2ISeriesTableDataEditor"        
        icon = "icons/table.gif"
        contributorClass = "org.eclipse.datatools.sqltools.data.internal.ui.editor.TableDataEditorActionBarContributor"
        name = "%TableDataEditor"
        id = "com.ibm.datatools.db2.iseries.internal.ui.explorer.actions.popup.actions.DB2ISeriesTableDataEditor" />          
  </extension> 
  <extension
         point="com.ibm.datatools.compare.ui.compareItemDependentItemProvider">
      <provider
            class="com.ibm.datatools.db2.iseries.ui.compare.DB2IndexEncodedVectorDependentItemProvider"
            compareItemName="encodedVector"
            objectTypes="DB2IndexImpl, ISeriesCatalogIndex">
      </provider>
        <provider
            class="com.ibm.datatools.db2.iseries.ui.compare.DB2IndexUniqueDependentItemProvider"
            compareItemName="unique"
            objectTypes="DB2IndexImpl, ISeriesCatalogIndex">
      </provider>
</extension>	
