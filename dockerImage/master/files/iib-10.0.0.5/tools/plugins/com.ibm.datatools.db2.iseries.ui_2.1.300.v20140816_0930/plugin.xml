   <!-- Content provider / Viewer to the Database Explorer -->
   <extension point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding viewerId="org.eclipse.datatools.connectivity.DataSourceExplorerNavigator">
         <includes>
            <contentExtension
                  pattern="com.ibm.datatools.iseries.server.ui.ServerExplorerContent"/>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding viewerId="org.eclipse.datatools.connectivity.DataSourceExplorerNavigator">
         <includes>
             <actionExtension pattern="com.ibm.datatools.db2.iseries.ui.actions.filter" />
         </includes>
      </viewerActionBinding> 
   </extension>
    
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            name="%com.ibm.datatools.db2.iseries.ui.contentProvider"
            priority="low"
            icon="platform:/plugin/org.eclipse.datatools.connectivity.sqm.core.ui/icons/server_explorer.gif"
            activeByDefault="true"
            contentProvider="com.ibm.datatools.db2.iseries.internal.ui.explorer.providers.content.impl.ISeriesServerExplorerContentProviderNav"
            labelProvider="com.ibm.datatools.db2.iseries.internal.ui.explorer.providers.label.ISeriesServerExplorerLabelProvider"			
            id="com.ibm.datatools.iseries.server.ui.ServerExplorerContent">
		    <triggerPoints>
		         <or>         
		            <instanceof 
		                  value="com.ibm.datatools.db2.iseries.ui.explorer.virtual.IISeriesMQTFolderNode" /> 
		            <instanceof
		                  value="com.ibm.db.models.db2.DB2Schema" /> 
		            <instanceof
		                  value="com.ibm.db.models.db2.DB2MaterializedQueryTable" /> 
		         </or>
		    </triggerPoints>
		    <commonSorter
            	id ="org.eclipse.datatools.connectivity.sqm.core.internal.ui.services.ExplorerSorterProvider"
            	class="org.eclipse.datatools.connectivity.sqm.core.internal.ui.services.ExplorerSorterProvider">
            	<parentExpression>
	           		<or>         
	            		<instanceof value="org.eclipse.datatools.modelbase.sql.schema.SQLObject"/>
                  		<instanceof value="org.eclipse.datatools.connectivity.sqm.core.ui.explorer.virtual.IVirtualNode"/> 
	            	</or>
         		</parentExpression>
            </commonSorter>
	        <possibleChildren>
	           <or>         
	            	<instanceof
	                  	value="org.eclipse.datatools.connectivity.sqm.core.internal.ui.explorer.virtual.IVirtualNode" /> 
	            	<instanceof 
	                  	value="org.eclipse.datatools.modelbase.sql.schema.SQLObject" />
	           </or>
	        </possibleChildren>
      </navigatorContent>
   </extension>
   
   <!-- Filter -->
	<extension
         point="org.eclipse.ui.navigator.navigatorContent">      		 
   		 <actionProvider
   			id = "com.ibm.datatools.db2.iseries.ui.actions.filter"
