<?eclipse version="3.0"?>
<plugin>

    <extension point="org.eclipse.help.contexts">
		<contexts file="help_contexts.xml"  plugin="com.ibm.j2ca.wmb.migration.core" />
	</extension>


  <!-- Here we define a new extension point.  This extension point is used
        to programmatically contribute adapter update entries -->
	<extension-point id="updateContributions" name="%Update_Contributions_Extension_Point.name" schema="schema/updateContributions.exsd"/>
	<extension-point id="migrationParticipant" name="%MigrationParticipantExtensionPoint.name" schema="schema/migrationParticipant.exsd"/>
	   <!-- Primary participant for adapter migration -->
	<extension
		point="com.ibm.j2ca.wmb.migration.core.migrationParticipant">
		<migrationParticipant
			class="com.ibm.j2ca.wmb.migration.AdapterUpdateParticipant"
            id="AdapterUpdateParticipant"
            name="%AdapterUpdateParticipantName">
		</migrationParticipant>
	</extension>
	
	<extension
		point="com.ibm.j2ca.wmb.migration.core.migrationParticipant">
		<migrationParticipant
			class="com.ibm.j2ca.wmb.migration.OrganizeClasspathParticipant"
            id="OrganizeClasspathParticipant"
            name="%OrganizeClasspathParticipantName">           
		</migrationParticipant>
	</extension>
	
</plugin>
