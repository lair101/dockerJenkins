       <moduleType
         types="jst.connector"
         versions="1.0, 1.5"/>
    </runtimeType>
  </extension>

  <extension point="org.eclipse.jst.server.core.runtimeClasspathProviders">
     <runtimeClasspathProvider
        id="com.ibm.etools.mbstub.runtime.runtimeTarget.v61"
