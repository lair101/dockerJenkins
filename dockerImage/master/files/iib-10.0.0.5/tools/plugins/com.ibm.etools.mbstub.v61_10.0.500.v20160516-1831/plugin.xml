<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension point="org.eclipse.wst.server.core.serverTypes">
      <serverType
            id="com.ibm.etools.mbstub.v61"
            name="%com.ibm.etools.mbstub"
            description="%com.ibm.etools.mbstub"
            runtime="true"
            startBeforePublish="true"
            supportsRemoteHosts="true"
            startTimeout="300000"
            stopTimeout="120000"
            launchConfigId="com.ibm.etools.mbstub.v61.core.launchConfigurationType"
