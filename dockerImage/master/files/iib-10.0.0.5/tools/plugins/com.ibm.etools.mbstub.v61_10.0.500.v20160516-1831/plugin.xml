  <extension point="org.eclipse.wst.server.core.internalStartup">
    <startup
      id="com.ibm.etools.mbstub.v61"
        class= "com.ibm.etools.mbstub.v61.internal.Startup"/>
  </extension>

   <extension
         point="org.eclipse.wst.server.ui.serverImages">
      <image
            icon="icons/obj16/serverV61_obj.gif"
            typeIds="com.ibm.etools.mbstub.v61, com.ibm.ws.ast.st.runtime.v61.mb"
            id="com.ibm.etools.mbstub.v61"/>
