        class="com.ibm.etools.mbstub.v61.internal.WASRuntimeClasspathProvider61"/>
  </extension>

  <extension point="org.eclipse.jst.server.core.runtimeFacetMappings">
    <runtimeFacetMapping
      runtimeTypeId="com.ibm.ws.ast.st.runtime.v61.mb"
      runtime-component="com.ibm.etools.mbstub.runtime"
      version="6.1"/>
  </extension>

<extension point="org.eclipse.wst.common.project.facet.core.runtimes">
    <adapter>
      <runtime-component
         id="com.ibm.etools.mbstub.runtime"/>
      <factory
         class="com.ibm.etools.mbstub.v61.internal.WASRuntimeClasspathProvider2$Factory"/>
      <type
         class="org.eclipse.jst.common.project.facet.core.IClasspathProvider"/>
    </adapter>
</extension>

  <extension point="org.eclipse.wst.common.project.facet.core.runtimes">
    <runtime-component-version
       type="com.ibm.etools.mbstub.runtime"
       version="6.1"/>
