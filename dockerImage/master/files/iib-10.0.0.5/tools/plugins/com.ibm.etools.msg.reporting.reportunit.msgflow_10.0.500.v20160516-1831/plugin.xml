<?eclipse version="3.0"?>
<plugin>
   
   <extension point = "com.ibm.etools.msg.reporting.infrastructure.reportunit">
   		<reportunit id="com.ibm.etools.msg.reporting.reportunit.msgflow"
   					displayName="%ReportUnit_Flow_name"
   					class="com.ibm.etools.msg.reporting.reportunit.msgflow.MsgflowReportUnit">
   			<resource fileExtension=".msgflow" resourceHandler="main"/>
   			<reportType name = "%ReportUnit_typeDetailed"/>
   		</reportunit>   		
   	</extension>  
   	
   	<extension point = "com.ibm.etools.msg.reporting.infrastructure.reportunit">
   		<reportunit id="com.ibm.etools.msg.reporting.reportunit.subflow"
   					displayName="%ReportUnit_Subflow_name"
   					class="com.ibm.etools.msg.reporting.reportunit.msgflow.MsgflowReportUnit">
   			<resource fileExtension=".subflow" resourceHandler="main"/>
   			<reportType name = "%ReportUnit_typeDetailed"/>
   		</reportunit>   		
   	</extension> 	   
</plugin>
