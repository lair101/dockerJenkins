   <extension
         id="mappingnature"
         name="%MappingNatureName"
         point="org.eclipse.core.resources.natures">
      <runtime>
        <run class="com.ibm.msl.mapping.xslt.codegen.builder.MappingNature"/>
      </runtime>
      <builder id="com.ibm.msl.mapping.xml.mappingbuilder"/>
   </extension>  
   <extension
         id="mappingbuilder"
         name="%MappingBuilderName"
         point="org.eclipse.core.resources.builders">
      <builder hasNature="true">
         <run
             class="com.ibm.msl.mapping.xslt.codegen.builder.MappingBuilder">
         </run>
      </builder>
   </extension>
   
   <!-- XML Map problem marker CheckSum and version synch problem marker -->
   <extension 
   		id="xmlMapProblemMarker" 
   		name="%XMLMapProblemMarker" 
   		point="org.eclipse.core.resources.markers">
   		<super type="org.eclipse.core.resources.problemmarker"/>
   		<attribute name="problemId"/>
   		<persistent value="true"/>
   	</extension>
    <extension 
   		id="xmlMapXSLT20ProblemMarker" 
   		name="%XMLMapProblemMarker" 
   		point="org.eclipse.core.resources.markers">
   		<super type="org.eclipse.core.resources.problemmarker"/>
   		<attribute name="problemId"/>
   		<persistent value="true"/>
   	</extension>
   	<!-- AKR - US55036 - categories extension point removed.
    <extension
         point="com.ibm.msl.mapping.categories">    
         <category id="com.ibm.msl.mapping.category.core" name="%categoryCore"/>
         <category id="com.ibm.msl.mapping.category.custom" name="%categoryCustom"/>
    </extension>
    -->
    
    
    <!-- AKR - US55036 -->
   <extension point = "com.ibm.msl.mapping.mappingEnvironments">
   		<mappingEnvironment class="com.ibm.msl.mapping.xml.domain.EclipseGDMMappingEnvironment"/>
   </extension>
   
   <extension
         point="com.ibm.msl.mapping.mappingDomains">
         <mappingDomain class="com.ibm.msl.mapping.xml.domain.XMLMappingDomain"/>
   </extension>
   
</plugin>
