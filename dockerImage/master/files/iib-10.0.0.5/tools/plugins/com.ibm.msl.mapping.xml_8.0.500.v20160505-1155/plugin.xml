   <extension-point id="resourceProviders" name="%resourceProviders" schema="schemas/resourceProviders.exsd"/>
   <extension-point id="transformEngines" name="%transformEngines" schema="schemas/transformEngines.exsd"/>
   
   <extension
         point="com.ibm.msl.mapping.xml.resourceProviders">
      <resourceProvider
            class="com.ibm.msl.mapping.xml.resources.XSDResourceProviderForXML" />
      <resourceProvider
            class="com.ibm.msl.mapping.xml.resources.XSDResourceProviderForDTD" />
      <resourceProvider
            class="com.ibm.msl.mapping.xml.resources.XSDResourceProviderForWSDL" />
      <resourceProvider
            class="com.ibm.msl.mapping.xml.resources.XSDResourceProviderForDFDL" />            
   </extension>
   
   <!-- XML Mapping Type Map Wrapper schema -->
   <extension point="org.eclipse.wst.xml.core.catalogContributions">
       <catalogContribution id="default">
			<public
				publicId="http://www.ibm.com/xmlmap"
				uri="xsd/xmlmapwrapper.xsd">
			</public>		
	   </catalogContribution>          
   </extension>  
   
   <!-- Mapping Launch Configurations -->
   <!-- AKR - 115006 - Add debug as a supported mode for this configuration -->
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            name="%MappingTransformation_launch"
            delegate="com.ibm.msl.mapping.xml.transform.launch.MappingLaunchConfigurationDelegate"
            modes="run,debug"
            id="com.ibm.msl.mapping.xml.MappingLauncher"
            public="false">
         <fileExtension
               default="true"
               extension="xquery">
         </fileExtension>
         <fileExtension
               default="true"
               extension="xsl">
         </fileExtension>         
      </launchConfigurationType>
   </extension>    
   
   <!-- ============================================================== -->   
   <!-- XSLT Extensions                                                -->
   <!-- ============================================================== -->
     <extension
           point="org.eclipse.core.contenttype.contentTypes">
        <file-association
              content-type="com.ibm.msl.mapping.mapping"
              file-extensions="map"/>
     </extension>
     
   <!-- XSLT1 Code Generator -->
   <!-- AKR - US55036
   <extension
         point="com.ibm.msl.mapping.codeGenerators"
         id="xslt">
      <codeGenerator
            class="com.ibm.msl.mapping.xml.generator.EclipseXMLMappingGenerator"/>
   </extension>
   -->
   
   <!-- XSLT2 Code Generator -->
   <!-- AKR - US55036
