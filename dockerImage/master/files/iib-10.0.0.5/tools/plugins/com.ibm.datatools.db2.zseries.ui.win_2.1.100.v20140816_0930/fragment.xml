<plugin>

   <extension point="org.eclipse.datatools.connectivity.ui.driverUIContributor">
      <driverUIContributor
            class="com.ibm.datatools.db2.zseries.internal.ui.connection.dialogs.DB2ZSeriesWinLDAPDriverUIContributor"
