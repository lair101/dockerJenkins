{
  "name": "iib-salesforce-connector",
  "version": "1.0.0",
  "description": "IIB Salesforce Connector",
  "main": "index.js",
  "scripts": {
    "lint": "jshint **.js",
    "lint-win64": "jshint . || exit 0",
    "test": "mocha test --timeout 10000",
    "cover": "istanbul cover $npm_package_config_cov_dir $npm_package_config_root_dir $NODE_DIR/lib/node_modules/mocha/bin/_mocha -- test --timeout 10000",
    "cover-win64": "istanbul cover %npm_package_config_cov_dir% %npm_package_config_root_dir%  %NODE_DIR%\\node_modules\\mocha\\bin\\_mocha -- test --timeout 10000",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --lines 100 --functions 100 $npm_package_config_cov_dir/coverage.json",
    "check-coverage-win64": "istanbul check-coverage --statements 100 --branches 100 --lines 100 --functions 100 %npm_package_config_cov_dir%\\coverage.json"
  },
  "author": "IBM"
}

