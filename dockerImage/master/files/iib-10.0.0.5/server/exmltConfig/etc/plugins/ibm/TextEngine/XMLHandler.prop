Name=IBM Text Engine XML Handler
stylesheetMutators=
Condition=!(x-ibm-wtp~no) & (content-type=text/xml)
Priority=2

#Property which indicates whether to check for stylesheets specified
#via Processing Instructions within the XML document BEFORE checking
#for WTP configured stylesheets.  Default is false, meaning to check
#for WTP configured stylesheets first before checking the document.
checkForDocumentStylesheetsFirst=false
